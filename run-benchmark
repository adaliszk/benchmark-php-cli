#!/usr/bin/env bash

echo "Run PHP 7.0 Minimal"
docker-compose up --build php70-minimal

echo "Run PHP 7.0 Complete"
docker-compose up --build php70-complete

echo "Run PHP 7.1 Minimal"
docker-compose up --build php71-minimal

echo "Run PHP 7.1 Complete"
docker-compose up --build php71-complete

echo "Run PHP 7.2 Minimal"
docker-compose up --build php72-minimal

echo "Run PHP 7.2 Complete"
docker-compose up --build php72-complete

chmod 777 -R ./output
